{
  "bugs": {
    "url": "https://github.com/evernaut/eslint-config-base/issues"
  },
  "dependencies": {},
  "description": "The Evernaut base ESLint config.",
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.4",
    "markdownlint": "^0.20.4",
    "markdownlint-cli": "^0.23.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "yaml-lint": "^1.2.4"
  },
  "engines": {
    "node": "12.16.3",
    "npm": "6.14.4"
  },
  "files": [
    "/src/**/*"
  ],
  "homepage": "https://www.evernaut.com",
  "keywords": [
    "eslint",
    "eslint-config",
    "evernaut",
    "evernaut-project-pan",
    "evernaut-type-eslint-config"
  ],
  "license": "MIT",
  "main": "./src/index.js",
  "name": "eslint-config-evernaut-base",
  "repository": {
    "type": "git",
    "url": "https://github.com/evernaut/eslint-config-base"
  },
  "scripts": {
    "lint": "npm-run-all -p lint:*",
    "lint:eslint": "eslint \"**/*.{js,json,ts}\" \"**/.*.{js,json,ts}\"",
    "lint:markdownlint": "markdownlint \"**/*.md\"",
    "lint:yamllint": "yamllint \"**/*.{yaml,yml}\" \"**/.*.{yaml,yml}\"",
    "test": "npm-run-all -p test:*",
    "test:lint": "npm run lint"
  },
  "version": "0.0.0"
}
